{# src/Okeanos/CoreBundle/Resources/views/Default/profile.html.twig #}
{% extends 'OkeanosCoreBundle::layout.html.twig' %}
{% block activePageLogin %} active{% endblock %}
{% block container %}
    <div class="row justify-content-center mt-3">
        <div class="col-sm-6">
            <p>Username : {{ app.user.username }}</p>
            <p>First name : {{ app.user.fname }}</p>
            <p>Last name : {{ app.user.lname }}</p>
        </div>
    </div>
{% endblock %}